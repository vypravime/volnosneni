:: PassageFooter

/*DEPENDENCIES: */
<<set _myPage = setup.DI_CONT.getService('myPage')>>
<<set _htmler = setup.DI_CONT.getService('htmler')>>
<<set _app = setup.DI_CONT.getService('app')>>
<<if _app.isDevMode()>>
  <<set _ajaxAnim = setup.DI_CONT.getService('ajaxAnimation')>>
<</if>>
/*\DEPENDENCIES */

<div class="tar">

  <<button "Nápověda">>
    <<passageDialog "Nápověda" "Help">>
  <</button>>&nbsp;
  
  <span @class="_htmler.getUIBarToggleBtnClass()">
    <<button "Panel ...">>
      <<run _myPage.triggerUIBarToggling()>>
    <</button>>
  <<run _htmler.READYfillUIBarToggleBtn() /*to sync on small screens, where bar is on start stowed*/>>
  </span>

</div>

<<if _app.isDevMode()>>
  <p class="tar mt6 mb0">
  
    <<button "AJAX ON">>
      <<run _ajaxAnim.setOn()>>
    <</button>>&nbsp;
    
    <<button "AJAX OFF">>
      <<run _ajaxAnim.setOff()>>
    <</button>>
  
  </p>
<</if>>

<div id="ajax-anim-right">
  <<include "AjaxAnim1">>
</div>